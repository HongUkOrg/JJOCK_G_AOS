<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/rosequartz</item>
        <item name="colorPrimaryDark">@color/rosequartz</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="TintTheme" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="colorAccent">@color/indigo</item>
    </style>
    <style name="CheckBoxTintTheme" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="colorAccent">@color/darkGreen</item>
    </style>

    <style name="EditTextTintTheme" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="colorAccent">@color/primaryText</item>
    </style>

    <style name="DialogTriggerButton">
        <item name="android:textAllCaps">true</item>
        <item name="android:layout_marginLeft">30dp</item>
        <item name="android:layout_marginRight">30dp</item>
        <item name="android:textColor">@android:color/white</item>
    </style>



</resources>
